	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Nik Barham",
			"email": "github@brokencube.co.uk"
		}
	],
	"require": {
		"php": ">= 5.6",
		"php-64bit": "*",
        	"psr/http-message": "1.0.1"
	},
	"require-dev": {
		"phpunit/phpunit": "4.3.*"
	},
  	"autoload": {
     	"psr-4": {
